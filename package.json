{
  "name": "vite-svelte-starter",
  "private": true,
  "version": "0.0.3",
  "main": "./src/main.js",
  "postcss": {
    "parser": "sugarss",
    "map": false,
    "plugins": {
      "postcss-plugin": {}
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "browserslist": "npx browserslist --coverage=DE,global --mobile-to-desktop --ignore-unknown-versions && npx browserslist --update-db",
    "tailwind": "npx tailwindcss -i ./src/index.css -o ./dist/index.css --watch",
    "preview": "vite preview",
    "format:prettier": "prettier --write --plugin-search-dir=. './src/**/*.svelte' --svelte-strict-mode=false",
    "format:eslint": "prettier-eslint --write './src/**/*.{css,html,js,svelte}'"
  },
  "devDependencies": {
    "@iconify/icons-akar-icons": "^1.2.1",
    "@iconify/icons-ant-design": "^1.2.1",
    "@iconify/icons-bx": "^1.2.1",
    "@iconify/icons-carbon": "^1.2.2",
    "@iconify/icons-codicon": "^1.2.3",
    "@iconify/icons-fluent": "^1.2.2",
    "@iconify/svelte": "^2.1.2",
    "@rollup/plugin-json": "^4.1.0",
    "@spaceavocado/svelte-form": "^1.0.4",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.2",
    "browserslist": "^4.19.3",
    "eslint": "^6.x.x",
    "eslint-plugin-svelte3": "^3.4.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.3",
    "postcss-nesting": "^10.1.2",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^7.4.1",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "prettier-plugin-svelte": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prettier-stylelint": "^0.4.2",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.49.9",
    "scss": "^0.2.4",
    "stylelint": "^14.5.3",
    "svelte": "^3.44.0",
    "svelte-json-tree": "^0.1.0",
    "svelte-preprocess": "^4.10.4",
    "svelte-reactive-preprocessor": "^0.8.0",
    "tailwindcss": "^3.0.23"
  }
}
